{
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "flowtype"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "semi": false,
      "singleQuote": true,
      "trailingComma": "es5",
      "bracketSpacing": false,
      "jsxBracketSameLine": true,
      "parser": "flow"
    }]
  },
  "extends": [
    "prettier",
    "plugin:flowtype/recommended"
  ]
}